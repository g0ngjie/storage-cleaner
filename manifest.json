{
  "manifest_version": 2,
  "name": "Storage cleaner",
  "version": "1.0.0",
  "description": "Lightweight tools for local storage & session storage",
  "author": "Gj",
  "icons": {
    "128": "images/128.png"
  },
  "permissions": [
    "storage",
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_icon": "images/128.png",
    "default_popup": "index.html"
  }
}